        },
        {
          "type": "minecraft:item",
          "name": "forbidden_arcanus:darkstone_upgrade_smithing_template"
        },
        {
          "type": "minecraft:item",
          "name": "minecraft:spire_armor_trim_smithing_template"
        },
        {
          "type": "minecraft:item",
